{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@app/*": [
        "./src/app/*"
      ],
      "@app/config/*": [
        "./src/app/config/*"
      ],
      "@app/store/*": [
        "./src/app/store/*"
      ],
      "@app/hooks/*": [
        "./src/app/hooks/*"
      ],
      "@app/types/*": [
        "./src/app/types/*"
      ],
      "@features/*": [
        "./src/features/*"
      ],
      "@public/*": [
        "./src/features/app/public/*"
      ],
      "@learner/*": [
        "./src/features/app/learner/*"
      ],
      "@errors/*": [
        "./src/features/app/errors/*"
      ],
      "@portfolio/*": [
        "./src/features/app/learner/features/portfolio/*"
      ],
      "@calendar/*": [
        "./src/features/app/learner/features/calendar/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@/store/*": [
        "./src/app/store/*"
      ],
      "@/hooks/*": [
        "./src/app/hooks/*"
      ],
      "@/configs/*": [
        "./src/app/config/*"
      ],
      "@/constants/*": [
        "./src/app/config/constants/*"
      ],
      "@/auth/*": [
        "./src/app/providers/auth/*"
      ],
      "@/auth": [
        "./src/app/providers/auth/index.ts"
      ],
      "@/views/*": [
        "./src/features/*"
      ],
      "@/@types/*": [
        "./src/app/types/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ]
    }
  },
  "include": [
    "src",
    "src/app/types"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}